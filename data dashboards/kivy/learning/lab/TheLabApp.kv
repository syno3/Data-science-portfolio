CanvasExample:

#:set s dp(150)

<CanvasExample>:
    canvas: 
        Rectangle:
            pos: dp(100), dp(100)
            size: s, s
        Ellipse:
            pos: 200, 500
            size: s, s/2
        Line:
            points: (0, 0, self.width/4, 100, self.width/2, 0,self.width*3/4, 100, self.width, 0)
            width: 2
